{
  "suggest.snippetIndicator": "üç¶",
  "coc.preferences.formatOnSaveFiletypes": ["css", "markdown", "javascript", "jsx", "typescript", "tsx"],
   "languageserver": {
    "clojure-lsp": {
      "command": "bash",
      "args": ["-c", "cd /Users/case/dev/lsp/ && lein run"],
      "filetypes": ["clojure"],
      "rootPatterns": ["project.clj"],
      "additionalSchemes": ["jar", "zipfile"],
      "trace.server": "verbose",
      "initializationOptions": {
      }
    },
    "clangd": {
      "command": "clangd",
      "args": ["--background-index"],
      "rootPatterns": ["compile_flags.txt", "compile_commands.json", ".git/", ".hg/"],
      "filetypes": ["c", "cpp", "objc", "objcpp"]
    },
    "golang": {
      "command": "gopls",
      "rootPatterns": ["go.mod"],
      "filetypes": ["go"]
    },
    "elmLS": {
      "command": "elm-language-server",
      "filetypes": ["elm"],
      "rootPatterns": ["elm.json"],
      "initializationOptions": {
        "elmPath": "elm", // optional
        "elmFormatPath": "elm-format", // optional
        "elmTestPath": "elm-test", // optional
        "elmAnalyseTrigger": "change" // optional
      }
    }
  }
}
